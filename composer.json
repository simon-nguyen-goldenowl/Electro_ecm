{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "almasaeed2010/adminlte": "~3.0",
        "bensampo/laravel-enum": "^3.2",
        "berkayk/onesignal-laravel": "^1.0",
        "elasticsearch/elasticsearch": "^7.0",
        "fabpot/goutte": "^4.0",
        "fideloper/proxy": "^4.4",
        "firebase/php-jwt": "^5.2",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.2",
        "jeroennoten/laravel-adminlte": "^3.5",
        "kreait/laravel-firebase": "^3.0",
        "laravel-notification-channels/fcm": "~2.0",
        "laravel-notification-channels/onesignal": "^2.3",
        "laravel-notification-channels/pusher-push-notifications": "^2.3",
        "laravel-notification-channels/webpush": "^5.0.2",
        "laravel/framework": "^8.12",
        "laravel/sanctum": "^2.8",
        "laravel/tinker": "^2.5",
        "laravel/ui": "^3.1",
        "loilo/fuse": "^4.0",
        "predis/predis": "^1.1",
        "pusher/pusher-php-server": "~4.0",
        "sendgrid/sendgrid": "^7.9",
        "wataridori/simple-fuzzy-search": "^0.1.1",
        "williamcruzme/laravel-fcm": "^1.0"
    },
    "require-dev": {
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "laravel/sail": "^0.0.5",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3.3",
        "squizlabs/php_codesniffer": "3.*"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
